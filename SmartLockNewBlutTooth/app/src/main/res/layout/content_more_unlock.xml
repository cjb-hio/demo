<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/content_more_unlock"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.xyw.smartlock.activity.MoreUnlockActivity">

    <include
        android:id="@+id/commontitlebar_morelock"
        layout="@layout/activity_commontitlebar"/>

    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swipe_layout_more_lock"
        android:layout_below="@+id/commontitlebar_morelock"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <com.yanzhenjie.recyclerview.swipe.SwipeMenuRecyclerView
            android:id="@+id/recycler_view_more_lock"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </android.support.v4.widget.SwipeRefreshLayout>

</RelativeLayout>
